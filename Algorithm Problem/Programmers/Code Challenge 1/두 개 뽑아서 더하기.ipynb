   "source": [
    "def solution(numbers):\n",
    "    sum_set = set([])\n",
    "    N = len(numbers)\n",
    "    for i in range(N-1):\n",
    "        for j in range(i+1, N):\n",
    "            sum_set.add(numbers[i] + numbers[j])\n",
    "    \n",
    "    return sorted(list(sum_set))"
   ]
