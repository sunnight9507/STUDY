    "n = int(input())\n",
    "\n",
    "cnt = 0\n",
    "while n:\n",
    "    tmp_cnt = 1\n",
    "    while True:\n",
    "        if tmp_cnt > n: break\n",
    "        n -= tmp_cnt\n",
    "        tmp_cnt += 1\n",
    "    cnt += tmp_cnt - 1\n",
    "    \n",
    "print(cnt)"
   ]
