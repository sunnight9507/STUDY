     "text": [
      "5\n"
     ]
    }
   ],
   "source": [
    "if N <= K: print(0)\n",
    "else:\n",
    "    lst.sort()\n",
    "    dis = [lst[i+1] - lst[i] for i in range(N-1)]\n",
    "    dis.sort(reverse=True)\n",
    "    print(sum(dis[K-1:]))"
   ]
  }
