   "source": [
    "### [점프 점프](https://www.acmicpc.net/problem/18512)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "7 7 2 1\n",
      "-1\n"
     ]
    }
   ],
   "source": [
    "X, Y, P1, P2 = map(int, input().split())\n",
    "\n",
    "_max = max(P1, P2) + (X * Y)\n",
    "while P1 <= _max and P2 <= _max:\n",
    "    if P1 == P2:\n",
    "        print(P1)\n",
    "        break\n",
    "    elif P1 > P2:\n",
    "        P2 += Y\n",
    "    else:\n",
    "        P1 += X\n",
    "else:\n",
    "    print(-1)"
   ]
  }
