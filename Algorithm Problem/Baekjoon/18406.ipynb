   "source": [
    "N = input()\n",
    "\n",
    "n = len(N) // 2\n",
    "left = N[:n]\n",
    "right = N[n:]\n",
    "left_sum = sum(map(int, left))\n",
    "right_sum = sum(map(int, right))\n",
    "\n",
    "if left_sum == right_sum:\n",
    "    print('LUCKY')\n",
    "else:\n",
    "    print('READY')"
   ]
  }
