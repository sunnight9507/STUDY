   "source": [
    "_str = input()\n",
    "find = input()\n",
    "\n",
    "cnt = 0\n",
    "n = len(find)\n",
    "idx = 0\n",
    "\n",
    "while idx < len(_str) - n + 1:\n",
    "    if _str[idx:idx+n] == find:\n",
    "        cnt += 1\n",
    "        idx += n - 1\n",
    "    idx += 1\n",
    "    \n",
    "print(cnt)"
   ]
  }
